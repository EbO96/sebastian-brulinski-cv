<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyViewPager">
        <attr name="enablePaging" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MyMaterialButton">
        <attr name="buttonColor" format="color" />
        <attr name="flat" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DotLevelView">
        <attr name="dotSize" format="dimension" />
        <attr name="dotMargin" format="dimension" />
        <attr name="levelDotColor" format="color" />
        <attr name="rangeDotColor" format="color" />
        <attr name="level" format="integer"/>
        <attr name="range" format="integer"/>
    </declare-styleable>

    <declare-styleable name="SlideDrawer">
        <attr name="mainContent" format="reference" />
        <attr name="closeButtonDrawable" format="reference" />
    </declare-styleable>

</resources>